{
  "expo": {
    "name": "MyEntertainmentList",
    "slug": "MyEntertainmentList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/app-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pucmm.mel",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "web": {
      "favicon": "./assets/app-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b20cc1f-2288-42aa-8e5f-6d074b1a4ae9"
      }
    }
  }
}
